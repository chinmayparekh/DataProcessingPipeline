<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <stage>
        <input type="excel">input/Students.xlsx</input>
        <task type="in-built">
            <function>slice</function>
            <condition>Name</condition>
        </task>
        <output>output/output_slice.txt</output>
        <sequenceId>1</sequenceId>
    </stage>
    <stage>
        <input type="excel">input/Students.xlsx</input>
        <task type="in-built">
            <function>dice</function>
            <condition>Name</condition>
            <condition>Age</condition>
        </task>
        <output>output/output_dice.txt</output>
        <sequenceId>2</sequenceId>
    </stage>
    <stage>
        <input type="excel">input/Students.xlsx</input>
        <task type="in-built">
            <function>remove_nulls</function>
        </task>
        <output>output/output_no_nulls.txt</output>
        <sequenceId>3</sequenceId>
    </stage>
    <stage>
        <input type="excel">input/Students.xlsx</input>
        <task type="in-built">
            <function>upper</function>
            <condition>Name</condition>
        </task>
        <output>output/output_upper.txt</output>
        <sequenceId>4</sequenceId>
    </stage>
    <stage>
        <input type="excel">input/Students.xlsx</input>
        <task type="in-built">
            <function>mean</function>
            <condition>Age</condition>
        </task>
        <output>output/output_mean.txt</output>
        <sequenceId>5</sequenceId>
    </stage>
</pipeline>